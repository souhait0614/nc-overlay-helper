{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": ["node_modules"],
  "include": [".plasmo/index.d.ts", "./**/*.ts", "./**/*.tsx"],
  "compilerOptions": {
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@NCOverlay/*": ["./NCOverlay/src/*"],
      "~*": ["./*"]
    }
  },
  "references": [
    {
      "path": "NCOverlay"
    }
  ]
}
